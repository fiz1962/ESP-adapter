{
  "name": "ESP-adapter",
  "version": "0.4.1",
  "description": "ESP Mozillia-IOT adapter",
  "main": "index.js",
  "keywords": [
    "mozilla",
    "iot",
    "adapter",
    "web",
    "server"
  ],
  "author": "Fiz N. Stein",
  "license": "MPL-2.0",
  "homepage": "https://github.com/fiz1962/ESP-adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiz1962/ESP-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/fiz1962/ESP-adapter/issues"
  },
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "esp-adapter.js",
    "index.js",
    "phpThings.tgz",
    "espThing.ino"
  ],
  "moziot": {
    "api": {
      "min": 1,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "ipStart": "10.0.2.10",
      "ipEnd": "10.0.2.20"
    },
    "schema": {
      "type": "object",
      "required": [
        "ipStart",
        "ipEnd"
      ],
      "properties": {
        "ipStart": {
          "type": "string",
          "pattern": "^(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])$"
        },
        "ipEnd": {
          "type": "string",
          "pattern": "^(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])$"
        }
      }
    }
  }
}
